{
  "root": "apps/big-data/infra",
  "sourceRoot": "apps/big-data/infra/src",
  "projectType": "application",
  "targets": {
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/big-data/infra/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/big-data/infra"],
      "options": {
        "jestConfig": "apps/big-data/infra/jest.config.js",
        "passWithNoTests": true
      }
    },
    "pu": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "pulumi up --yes --stack=master",
        "cwd": "apps/big-data/infra"
      }
    },
    "pd": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "pulumi destroy --yes --stack=master -- --verbose",
        "cwd": "apps/big-data/infra"
      }
    }
  },
  "tags": []
}
