{
  "root": "apps/fullstack/event2",
  "sourceRoot": "apps/fullstack/event2/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/fullstack/event2",
        "main": "apps/fullstack/event2/src/main.ts",
        "tsConfig": "apps/fullstack/event2/tsconfig.app.json",
        "assets": ["apps/fullstack/event2/src/assets"],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/fullstack/event2/src/environments/environment.ts",
              "with": "apps/fullstack/event2/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "fullstack-event2:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/fullstack/event2/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/fullstack/event2"],
      "options": {
        "jestConfig": "apps/fullstack/event2/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "name=event2 src=apps/fullstack/event2 bash scripts/deploy-to-cloud-run.sh"
      }
    }
  },
  "tags": []
}
