{
  "root": "apps/fullstack/users",
  "sourceRoot": "apps/fullstack/users/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nrwl/node:build",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "apps/fullstack/users/dist",
        "main": "apps/fullstack/users/src/main.ts",
        "tsConfig": "apps/fullstack/users/tsconfig.app.json",
        "assets": [
          "apps/fullstack/users/src/assets"
        ],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false,
          "fileReplacements": [
            {
              "replace": "apps/fullstack/users/src/environments/environment.ts",
              "with": "apps/fullstack/users/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:execute",
      "options": {
        "buildTarget": "fullstack-users:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "apps/fullstack/users/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "coverage/apps/fullstack/users"
      ],
      "options": {
        "jestConfig": "apps/fullstack/users/jest.config.js",
        "passWithNoTests": true
      }
    },
    "deploy": {
      "executor": "@nrwl/workspace:run-commands",
      "outputs": [],
      "options": {
        "command": "name=users src=apps/fullstack/users bash scripts/deploy-to-cloud-run.sh"
      }
    }
  },
  "tags": []
}
